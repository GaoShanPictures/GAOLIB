   <string notr="true">QMainWindow{
    color: #b1b1b1;
    background-color: #3c3c3c;
}

QWidget{
            background-color: #444;
            color: white;
}

QScrollArea{
            background-color: #444;
            color: white;
			border: 0px solid #222;
			
}

QListWidget{
            background-color: #444;
            color: white;
			border: 0px solid #222;
			
}

QStatusBar{
    color: white;
}

QMenuBar {
            background-color: #444;
}

QMenuBar::item {
            background-color: #444;
            color: rgb(255,255,255);
}

QMenuBar::item::selected {
            background-color: rgb(30,30,30);
}

QMenu {
            background-color: #3c3c3c;
}

QMenu::item {
            background-color: #3c3c3c;
