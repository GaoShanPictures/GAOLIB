            background-color: rgb(30,30,30);
}

QDialog
{
    color: #b1b1b1;
    background-color: #3c3c3c;
}

QTabWidget QWidget
{
    background: #444;
}

#page, #page_2, #page_3 {
    background-color: #3c3c3c;
}

QGroupBox {
     color: #628CB2;
     border: 1px solid #222;
     border-radius: 5px;
     margin-top: 1ex; /* leave space at the top for the title */
}

QGroupBox::title {
     subcontrol-origin: margin;
     subcontrol-position: top center; /* position at the top center */
     padding: 0 3px;
     background-color: QLinearGradient( x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #333, stop: 1 #3c3c3c);
}

QHeaderView::section:vertical
{
    margin-bottom: 4px;
    border: 1px solid;
}

QLineEdit
{
    background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #4d4d4d, stop: 0 #646464, stop: 1 #5d5d5d);
    padding: 1px;
    border-style: solid;
    border: 1px solid #4D5056;
    border-radius: 5;
    color: #b1b1b1;
}

QPushButton
{
    color: #b1b1b1;
    background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #565656, stop: 0.1 #525252, stop: 0.5 #4e4e4e, stop: 0.9 #4a4a4a, stop: 1 #464646);
    border-width: 1px;
    border-color: #555555;
    border-style: solid;
    border-radius: 5;
    padding: 3px;
    padding-left: 5px;
    padding-right: 5px;
    outline: 0;
}

QPushButton:pressed
{
    background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #3d3d3d, stop: 0.1 #3b3b3b, stop: 0.5 #393939, stop: 0.9 #383838, stop: 1 #353535);
}

QComboBox
{
    color: #b1b1b1;
    background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #565656, stop: 0.1 #525252, stop: 0.5 #4e4e4e, stop: 0.9 #4a4a4a, stop: 1 #464646);
    border: 1px outset #4D5056;
    border-radius: 3px;
    padding: 1px 3px 1px 5px;
    selection-background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #097ace, stop: 1 #097ace);
    combobox-popup: 1;
}

QComboBox:hover, QPushButton:hover
{
    border: 1px solid QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #333 , stop: 1 #333);
}

QComboBox:on
{
    background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #2d2d2d, stop: 0.1 #2b2b2b, stop: 0.5 #292929, stop: 0.9 #282828, stop: 1 #252525);
}

QComboBox:!editable:on, QComboBox::drop-down:editable:on {
    background: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #2d2d2d, stop: 0.1 #2b2b2b, stop: 0.5 #292929, stop: 0.9 #282828, stop: 1 #252525);
}

QComboBox::drop-down
{
     subcontrol-origin: padding;
     subcontrol-position: top right;
     border-left-width: 0px;
     border-left-color: black;
     border-left-style: inset;
     border-top-right-radius: 3px; /* same radius as the QComboBox */
     border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
     image: url(./icons/arrow.png);
}

QComboBox QAbstractItemView {
    selection-background-color: lightgray;
    background-color: #3c3c3c;
    border: 1px outset #3c3c3c;
}

QLabel
{
    color: #b1b1b1;
}

QSplitter::handle:vertical
{
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #494949, stop:1 #6f6f6f);
    height: 2px;
}

QSplitter::handle:horizontal
{
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop:0 #494949, stop:1 #6f6f6f);
    width: 2px;
}

QToolBox {
    background: #444;
}

QToolBox QScrollArea&gt;QWidget&gt;QWidget
{
    background: #444;
}

QToolBox::tab {
   background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #E1E1E1, stop: 0.4 #DDDDDD, stop: 0.5 #D8D8D8, stop: 1.0 #D3D3D3);
   color: darkgray;
}

QToolBox::tab:selected {
    color: black;
}

QTabWidget::pane { /* The tab widget frame */
    border: 1px solid #333;
    background-color: #3c3c3c;
}

QTabBar::tab {
    background: #3a3a3a;
    border: 1px solid #222;
    border-bottom-color: #444; /* same as the pane color */
    min-width: 8ex;
    border-top-left-radius: 2px;
    border-top-right-radius: 2px;
    padding: 3px;
}


QTabBar::tab:selected, QTabBar::tab:hover {
    background: #444;
    border-bottom-color: #444; /* same as the pane color */
    color: white;
}

QTabBar::tab:!selected {
    margin-top: 2px; /* make non-selected tabs look smaller */
}

QPlainTextEdit {
    background-color: #3c3c3c;
    border: 0px solid #222;
}</string>
